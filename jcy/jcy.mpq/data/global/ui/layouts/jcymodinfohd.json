                        "textColor": "$LightButtonTextColor",
                        "text/style" : "$StyleFEButtonText",
                        "textString": "凯恩之角"
                    }
                },
				{
                    "type": "ButtonWidget", "name": "gitInfo",
                    "fields": {
                        "rect": { "x": 510, "y": 400 },
